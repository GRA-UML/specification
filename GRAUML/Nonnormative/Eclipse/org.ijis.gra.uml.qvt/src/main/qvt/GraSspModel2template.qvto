		if(result.name.oclIsUndefined() or (result.name=''))then{
			name:=messagetype.name;
		}endif;
	}endif;
	message:=messagetype.mapImplicitMessage(self,portDefaults,interfaceDefaults,operationDefaults,parameterDefaults,messageDefaults);
}	
