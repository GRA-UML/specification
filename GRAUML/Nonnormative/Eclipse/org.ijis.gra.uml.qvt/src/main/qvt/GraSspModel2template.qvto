	minorVersion:=appliedStereotype.slot->select(s|s.definingFeature.name='MinorVersion').value.oclAsType(UML::LiteralInteger).value->asSequence()->first();
	if(result.majorVersion.oclIsUndefined())then{
		majorVersion:=1;
	}endif;
