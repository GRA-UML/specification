          <xsd:documentation>A formal contract associated with a service, application, process, tranaction, or thing, tangible or otherwise.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="licensingAgreement" type="data.grauml.graAnnotationModel:Agreement">
        <xsd:annotation>
          <xsd:documentation>A type of licensing for a service or application. Descriptive values could be In House, No License,  Open Source, Purchase License, etc.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="usageCostAmount" type="data.grauml.graAnnotationModel:AmountType">
        <xsd:annotation>
          <xsd:documentation>A total cost to use something, such as a service, etc. </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="usageUnitCostAmount" type="data.grauml.graAnnotationModel:AmountType">
        <xsd:annotation>
          <xsd:documentation>A cost associated with a service(e.g. transaction, unlimited transactions, minutes of use).</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="creationCostAmount" type="data.grauml.graAnnotationModel:AmountType">
        <xsd:annotation>
          <xsd:documentation>A cost create a thing, such as an application or service  This includes the full cost to design, manage, develop, test, implement and maintain.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element ref="xmi:Extension"/>
    </xsd:choice>
    <xsd:attribute ref="xmi:id"/>
    <xsd:attributeGroup ref="xmi:ObjectAttribs"/>
    <xsd:attribute name="applicablePoliciesIndicator" type="xsd:string" use="required">
      <xsd:annotation>
        <xsd:documentation>True when there are any applicable policies governing the use, administration, or implementation of a service; False otherwise.</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="applicableContractsIndicator" type="xsd:string" use="required">
      <xsd:annotation>
        <xsd:documentation>True when there are any applicable contracts relating to the use, administration, or implementation of a service; False otherwise.</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="applicableAgreementsIndicator" type="xsd:string" use="required">
      <xsd:annotation>
        <xsd:documentation>True when there are any applicable agreements or Memoranda Of Understanding (MOUs) relating to the use, administration, or implementation of a service; False otherwise.</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="applicableUmbrellaAgreementsIndicator" type="xsd:string" use="required">
      <xsd:annotation>
        <xsd:documentation>True when there are any applicable umbrella agreements relating to the use, administration, or implementation of a service; False otherwise.</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="applicablePolicies" type="xsd:string">
      <xsd:annotation>
        <xsd:documentation>A collection of all policies that in some way constrain, govern, or control the usage of a service, application, process. etc.</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="approvalRequiredIndicator" type="xsd:string" use="required">
      <xsd:annotation>
        <xsd:documentation>True when a permission must first be obtained prior to using a service or performing some action in a business process; False otherwise.</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="licensingRequiredIndicator" type="xsd:string" use="required">
      <xsd:annotation>
        <xsd:documentation>True when a license is required to use a service or application; False otherwise.</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="serviceResponseTime" type="xsd:string" use="required">
      <xsd:annotation>
        <xsd:documentation>A description of the average response time for a service.  The response time is calculated as the time input is provided to the service until the service completes its process or provides output for the consumer.</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="serviceAverageThroughput" type="xsd:string">
      <xsd:annotation>
        <xsd:documentation>A description of how often a service is expected to be, or actually used, averaged over a period of time.</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="serviceMaximumThroughput" type="xsd:string">
      <xsd:annotation>
        <xsd:documentation>A description of the limit of how often a service is able to be accessed or used at, over a period of time during peak capacity.</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="serviceAvailability" type="xsd:string">
      <xsd:annotation>
        <xsd:documentation>A description or measurement of the expected availbility that a service is usable.</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
  </xsd:complexType>
  <xsd:element name="ServiceLevelAgreement" type="data.grauml.graAnnotationModel:ServiceLevelAgreement">
    <xsd:annotation>
      <xsd:documentation>A collection of policies, agreements, licensing and any other goverenace or performance documentation specifying constraints and any other details regarding the realization of a service.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:complexType name="AmountType">
    <xsd:annotation>
      <xsd:documentation>A data type for an amount of money.</xsd:documentation>
    </xsd:annotation>
    <xsd:choice maxOccurs="unbounded" minOccurs="0">
      <xsd:element ref="xmi:Extension"/>
    </xsd:choice>
    <xsd:attribute ref="xmi:id"/>
    <xsd:attributeGroup ref="xmi:ObjectAttribs"/>
    <xsd:attribute name="currencyText" type="xsd:string">
      <xsd:annotation>
        <xsd:documentation>A unit of money or exchange.</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="value" type="xsd:string" use="required">
      <xsd:annotation>
        <xsd:documentation>The value of the amount, expressed in units defined by currencyText.</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
  </xsd:complexType>
  <xsd:element name="AmountType" type="data.grauml.graAnnotationModel:AmountType">
    <xsd:annotation>
      <xsd:documentation>A data type for an amount of money.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:complexType name="ServiceInterfaceSpecification">
    <xsd:annotation>
      <xsd:documentation>A data type representing details relating to the interface of a service.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="data.grauml.graAnnotationModel:GraServiceAnnotationBase">
        <xsd:choice maxOccurs="unbounded" minOccurs="0">
          <xsd:element name="messageDefinitionMechanism" nillable="true" type="xsd:string">
            <xsd:annotation>
              <xsd:documentation>This propoerty includes information about the message definition mechanism utilized by the service actions.

Per the GRA, message definition mechanisms are closely related to the interface description requirements.  Unlike interface description requirements, message definition mechanisms establish a standard way of defining the structure and contents of a message.

Additional artifacts related to this section's content can be provided in the artifacts folder of the service package.
  
[Service Abbreviation] SSP [Service Version]\artifacts

If such artifacts are provided,  they should be referenced in this section. A description of the artifact and a link to it should be provided as part of the reference.
</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="serviceInteractionProfile" type="data.grauml.graAnnotationModel:ServiceInteractionProfile">
            <xsd:annotation>
              <xsd:documentation>A set of information about the Service Interaction Profile Implemented.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="portDefault" type="data.grauml.graAnnotationModel:Port">
            <xsd:annotation>
              <xsd:documentation>Default port specification for all ports of all service components realized by contained Services.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="interfaceDefault" type="data.grauml.graAnnotationModel:Interface">
            <xsd:annotation>
              <xsd:documentation>Default interface specification for all provided interfaces of ports of all service components realized by contained Services.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="operationDefault" type="data.grauml.graAnnotationModel:Operation">
            <xsd:annotation>
              <xsd:documentation>Default operation specification for all operations owned by provided interfaces of ports of all service components realized by contained Services.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="messageDefault" type="data.grauml.graAnnotationModel:Message">
            <xsd:annotation>
              <xsd:documentation>Default message specification for all parameters of all operations owned by provided interfaces of ports of all service components realized by contained Services.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="provider" type="data.grauml.graAnnotationModel:Service"/>
        </xsd:choice>
        <xsd:attribute name="securityImplementedIndicator" type="xsd:string" use="required">
          <xsd:annotation>
            <xsd:documentation>True when security has been implemented to access this service.False otherwise.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="securityDescriptionText" type="xsd:string">
          <xsd:annotation>
            <xsd:documentation>A text description that identifies the security which was implemented to protect this service interface (GFIPM, Trusted Broker, etc)</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="uriAddress" type="xsd:string">
          <xsd:annotation>
            <xsd:documentation>URI of the (WSDL) service interface potentially including version and environment.
[derived from] Element name.
The URI is relative to the SSP Catalog.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="uriDescription" type="xsd:string">
          <xsd:annotation>
            <xsd:documentation>A description of the URI. This would provide better understanding of what is actually at the URI address.
[derived from] Element description</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="serviceInterfaceDescriptionUri" type="xsd:string">
          <xsd:annotation>
            <xsd:documentation>URI of the Service Interface Description document, potentially including version and environment.
[derived from] Element name.
The URI is relative to the SSP Catalog.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="targetNamespace" type="xsd:string" use="required">
          <xsd:annotation>
            <xsd:documentation>Target namespace of the (WSDL) Service Interface.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="prefix" type="xsd:string" use="required">
          <xsd:annotation>
            <xsd:documentation>Prefix to be used for target namespace of (WSDL) Service Specification.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:element name="ServiceInterfaceSpecification" type="data.grauml.graAnnotationModel:ServiceInterfaceSpecification">
    <xsd:annotation>
      <xsd:documentation>A data type representing details relating to the interface of a service.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:complexType name="ServiceInteractionProfile">
    <xsd:annotation>
      <xsd:documentation>A data structure containing information about the Service Interaction Profile Implemented</xsd:documentation>
    </xsd:annotation>
    <xsd:choice maxOccurs="unbounded" minOccurs="0">
      <xsd:element ref="xmi:Extension"/>
    </xsd:choice>
    <xsd:attribute ref="xmi:id"/>
    <xsd:attributeGroup ref="xmi:ObjectAttribs"/>
    <xsd:attribute name="sipName" type="xsd:string" use="required">
      <xsd:annotation>
        <xsd:documentation>A name of the Service Interaction Profile Implemented.
[Derived from] SIP Name</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="sipVersion" type="xsd:string" use="required">
      <xsd:annotation>
        <xsd:documentation>A version of the Service Interaction Profile Implemented</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
  </xsd:complexType>
  <xsd:element name="ServiceInteractionProfile" type="data.grauml.graAnnotationModel:ServiceInteractionProfile">
    <xsd:annotation>
      <xsd:documentation>A data structure containing information about the Service Interaction Profile Implemented</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
</xsd:schema>
