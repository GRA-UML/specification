      <xsd:documentation>[Derived from] Use case or package with use cases</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:complexType name="Agreement">
    <xsd:annotation>
      <xsd:documentation>A data structure representing an agreement, formal or informal, associated with a service, application or thing, tangible or otherwise.</xsd:documentation>
    </xsd:annotation>
    <xsd:choice maxOccurs="unbounded" minOccurs="0">
      <xsd:element name="condition" nillable="true" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>Specification of the condition for the agreement.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="obligation" nillable="true" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>Specification of the obligation associated withr the agreement.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="constraint" nillable="true" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>Specification of the constraint associated with the agreement.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element ref="xmi:Extension"/>
    </xsd:choice>
    <xsd:attribute ref="xmi:id"/>
    <xsd:attributeGroup ref="xmi:ObjectAttribs"/>
    <xsd:attribute name="agreementDescriptionText" type="xsd:string">
      <xsd:annotation>
        <xsd:documentation>A free text description of an agreement, formal or informal, associated with a service, application or thing, tangible or otherwise.
[Derived from] Element Documentation</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="agreementUri" type="xsd:string">
      <xsd:annotation>
        <xsd:documentation>A locator referencing an agreement, formal or informal, associated with a service, application or thing, tangible or otherwise.</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="automatedAgreementIndicator" type="xsd:string">
      <xsd:annotation>
        <xsd:documentation>Indicates whether the policy or contract represented by the Agreement has an automated implementation.</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
  </xsd:complexType>
  <xsd:element name="Agreement" type="data.grauml.graAnnotationModel:Agreement">
    <xsd:annotation>
      <xsd:documentation>A data structure representing an agreement, formal or informal, associated with a service, application or thing, tangible or otherwise.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:complexType name="IepdReference">
