          <xsd:documentation>An umbrella agreement that in some way constrain, govern, or control the usage of a service, application, process. etc.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="agreement" type="data.grauml.graAnnotationModel:Agreement">
