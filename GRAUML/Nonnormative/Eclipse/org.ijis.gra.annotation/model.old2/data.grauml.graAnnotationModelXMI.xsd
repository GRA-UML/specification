  <xsd:element name="Description" type="data.grauml.graAnnotationModel:Description">
    <xsd:annotation>
      <xsd:documentation>Documentation for any element contained in element documentation</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:complexType name="ServiceLevelAgreement">
