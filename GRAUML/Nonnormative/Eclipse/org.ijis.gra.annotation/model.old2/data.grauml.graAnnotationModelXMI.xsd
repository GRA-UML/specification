  <xsd:complexType name="Service">
    <xsd:annotation>
      <xsd:documentation>Realizes component</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="data.grauml.graAnnotationModel:Participant">
        <xsd:choice maxOccurs="unbounded" minOccurs="0">
          <xsd:element name="interfaceDefault" type="data.grauml.graAnnotationModel:Interface">
            <xsd:annotation>
              <xsd:documentation>Default interface specification provided for all ports owned by the service component.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="messageDefault" type="data.grauml.graAnnotationModel:Message">
            <xsd:annotation>
              <xsd:documentation>The default message specification for all parameters of all operations of all interfaces provided by the owned ports of the service component.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="operationDefault" type="data.grauml.graAnnotationModel:Operation">
            <xsd:annotation>
              <xsd:documentation>The default operation specification for all operations owned by the interfaces provided by the owned ports of the realized service component.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="port" type="data.grauml.graAnnotationModel:Port">
            <xsd:annotation>
              <xsd:documentation>The port specification for the ports owned by the service component realizing this Service. </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:choice>
      </xsd:extension>
    </xsd:complexContent>
