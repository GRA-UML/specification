      <xsd:documentation>Realizes component</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:complexType name="Port">
    <xsd:annotation>
      <xsd:documentation>Represents a service port, realizes a UML Port on the Service Component.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="data.grauml.graAnnotationModel:GraServiceAnnotationBase">
        <xsd:choice maxOccurs="unbounded" minOccurs="0">
          <xsd:element name="interface" type="data.grauml.graAnnotationModel:Interface">
            <xsd:annotation>
              <xsd:documentation>Interface realized by the Port.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="operationDefault" type="data.grauml.graAnnotationModel:Operation">
            <xsd:annotation>
              <xsd:documentation>Default operation specification for operations within interfaces provided by this Port.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="messageDefault" type="data.grauml.graAnnotationModel:Message">
            <xsd:annotation>
              <xsd:documentation>Default message specification for operation parameters within interfaces provided by this Port.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:choice>
        <xsd:attribute name="addressUri" type="xsd:string">
          <xsd:annotation>
            <xsd:documentation>The &lt;soap:address&gt; location of a &lt;wsdl:port&gt;.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:element name="Port" type="data.grauml.graAnnotationModel:Port">
    <xsd:annotation>
      <xsd:documentation>Represents a service port, realizes a UML Port on the Service Component.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
