          <xsd:element name="serviceLevelAgreement" type="data.grauml.graAnnotationModel:ServiceLevelAgreement">
            <xsd:annotation>
              <xsd:documentation>A collection of policies, agreements, licensing and any other goverenace or performance documentation specifying constraints and any other details regarding the realization of a service.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="relatedOrganization" type="data.grauml.graAnnotationModel:Organization">
            <xsd:annotation>
              <xsd:documentation>A collection of organizations that are somehow related to the service.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="serviceInterface" type="data.grauml.graAnnotationModel:ServiceInterfaceSpecification">
            <xsd:annotation>
              <xsd:documentation>A set of details relating to the interface of a service.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="realWorldEffect" type="data.grauml.graAnnotationModel:UseCase">
            <xsd:annotation>
              <xsd:documentation>A Documented Component</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="serviceInteraction" type="data.grauml.graAnnotationModel:ServiceInteraction">
            <xsd:annotation>
              <xsd:documentation>A Documented Component</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:choice>
        <xsd:attribute name="classification" type="xsd:string">
          <xsd:annotation>
            <xsd:documentation>A collection of classifications defining the relationship between a service and an applicable enterprise architecture and business reference model.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="sponsors" type="xsd:string">
          <xsd:annotation>
            <xsd:documentation>A collection of professional or governmental organization(s) or person that sponsored, contributed, or participated in the development of a service.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="serviceDescriptionSummaryText" type="xsd:string">
          <xsd:annotation>
            <xsd:documentation>A brief summary of this service for short display purposes.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="domainDescription" type="xsd:string">
          <xsd:annotation>
            <xsd:documentation>A primary domain or line of business (LoB) that a service covers.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="transformationUri" type="xsd:string">
          <xsd:annotation>
            <xsd:documentation>URI of an XSLT that transforms the skeleton SSP into a final one as the "phase-2" transform. &#13;
A Documented Component</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="majorVersion" type="xsd:string" use="required">
          <xsd:annotation>
            <xsd:documentation>A value identifying the primary version number.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="minorVersion" type="xsd:string" use="required">
          <xsd:annotation>
            <xsd:documentation>A value designating a minor version number.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="revisionVersion" type="xsd:string">
          <xsd:annotation>
            <xsd:documentation>A value designating a minor version revision number.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="creationDate" type="xsd:string" use="required">
          <xsd:annotation>
            <xsd:documentation>A date designation when a service was first created. Do NOT to be confuse with the date a service is submitted to a registry. The format is YYYY-MM. Set by default.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="activationDate" type="xsd:string">
          <xsd:annotation>
            <xsd:documentation>A date when a service was or will be first available in production. Do NOT confuse with the date you submitted this service to a registry. The format is YYYY-MM.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="lastRevisionDate" type="xsd:string">
          <xsd:annotation>
            <xsd:documentation>A date with the year and month specifying when this service information was last revised. Do NOT confuse with the date the service itself was last revised with generating new service documentation. The format is YYYY-MM. Set by default.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="nextRevisionDate" type="xsd:string">
          <xsd:annotation>
            <xsd:documentation>A year and month (YYYY-MM) a service is expected to be revised.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="expirationDate" type="xsd:string">
          <xsd:annotation>
            <xsd:documentation>A year and month (YYYY-MM) this service is expected to be no longer available (if applicable).</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="lifecycleStatus" type="xsd:string" use="required">
          <xsd:annotation>
            <xsd:documentation>A word indicating the current stage of the service within the development lifecycle.  Valid values are; In Design, In Development, Release Candidate, Operational/Production, Deprecated</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:element name="ServiceDescription" type="data.grauml.graAnnotationModel:ServiceDescription">
    <xsd:annotation>
      <xsd:documentation>A data structure representing the details describing a service.
[Realizes] Collaboration
[Uses] IEPD</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:complexType name="ServiceIdentification">
    <xsd:annotation>
      <xsd:documentation>A data structure representing the means of uniquely identifying a service.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="data.grauml.graAnnotationModel:GraServiceAnnotationBase">
        <xsd:choice maxOccurs="unbounded" minOccurs="0">
          <xsd:element name="serviceId" nillable="true" type="xsd:string">
            <xsd:annotation>
              <xsd:documentation>An identification of the service in a service registry and/or repository.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="serviceUri" nillable="true" type="xsd:string">
            <xsd:annotation>
              <xsd:documentation>A fully qualified locator of the service interface potentially including version and environment.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="serviceNameAbbreviationText" nillable="true" type="xsd:string">
            <xsd:annotation>
              <xsd:documentation>A human readable abbreviation of the Service Name.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:choice>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:element name="ServiceIdentification" type="data.grauml.graAnnotationModel:ServiceIdentification">
    <xsd:annotation>
      <xsd:documentation>A data structure representing the means of uniquely identifying a service.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:complexType name="Description">
    <xsd:annotation>
      <xsd:documentation>Documentation for any element contained in element documentation</xsd:documentation>
    </xsd:annotation>
    <xsd:choice maxOccurs="unbounded" minOccurs="0">
      <xsd:element ref="xmi:Extension"/>
    </xsd:choice>
    <xsd:attribute ref="xmi:id"/>
    <xsd:attributeGroup ref="xmi:ObjectAttribs"/>
    <xsd:attribute name="description" type="xsd:string" use="required">
      <xsd:annotation>
        <xsd:documentation>Description derived from element description&#13;
A Documented Component</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="externalDocumentation" type="xsd:string">
      <xsd:annotation>
        <xsd:documentation>A Documented Component
A Documented Component</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
  </xsd:complexType>
  <xsd:element name="Description" type="data.grauml.graAnnotationModel:Description">
    <xsd:annotation>
      <xsd:documentation>Documentation for any element contained in element documentation</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:complexType name="ServiceLevelAgreement">
    <xsd:annotation>
      <xsd:documentation>A collection of policies, agreements, licensing and any other goverenace or performance documentation specifying constraints and any other details regarding the realization of a service.</xsd:documentation>
    </xsd:annotation>
    <xsd:choice maxOccurs="unbounded" minOccurs="0">
      <xsd:element name="umbrellaAgreement" type="data.grauml.graAnnotationModel:Agreement">
        <xsd:annotation>
          <xsd:documentation>An umbrella agreement that in some way constrain, govern, or control the usage of a service, application, process. etc.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="agreement" type="data.grauml.graAnnotationModel:Agreement">
        <xsd:annotation>
          <xsd:documentation>A description or reference to an applicable Agreement or MOU governing the use, administration, or implementation of a service.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="applicableContract" type="data.grauml.graAnnotationModel:Agreement">
        <xsd:annotation>
          <xsd:documentation>A formal contract associated with a service, application, process, tranaction, or thing, tangible or otherwise.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="licensingAgreement" type="data.grauml.graAnnotationModel:Agreement">
        <xsd:annotation>
          <xsd:documentation>A type of licensing for a service or application. Descriptive values could be In House, No License,  Open Source, Purchase License, etc.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="usageCostAmount" type="data.grauml.graAnnotationModel:AmountType">
        <xsd:annotation>
          <xsd:documentation>A total cost to use something, such as a service, etc. </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="usageUnitCostAmount" type="data.grauml.graAnnotationModel:AmountType">
        <xsd:annotation>
          <xsd:documentation>A cost associated with a service(e.g. transaction, unlimited transactions, minutes of use).</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="creationCostAmount" type="data.grauml.graAnnotationModel:AmountType">
        <xsd:annotation>
          <xsd:documentation>A cost create a thing, such as an application or service  This includes the full cost to design, manage, develop, test, implement and maintain.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element ref="xmi:Extension"/>
    </xsd:choice>
    <xsd:attribute ref="xmi:id"/>
    <xsd:attributeGroup ref="xmi:ObjectAttribs"/>
    <xsd:attribute name="applicablePoliciesIndicator" type="xsd:string" use="required">
      <xsd:annotation>
        <xsd:documentation>True when there are any applicable policies governing the use, administration, or implementation of a service; False otherwise.</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="applicableContractsIndicator" type="xsd:string" use="required">
      <xsd:annotation>
        <xsd:documentation>True when there are any applicable contracts relating to the use, administration, or implementation of a service; False otherwise.</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="applicableAgreementsIndicator" type="xsd:string" use="required">
      <xsd:annotation>
        <xsd:documentation>True when there are any applicable agreements or Memoranda Of Understanding (MOUs) relating to the use, administration, or implementation of a service; False otherwise.</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="applicableUmbrellaAgreementsIndicator" type="xsd:string" use="required">
      <xsd:annotation>
        <xsd:documentation>True when there are any applicable umbrella agreements relating to the use, administration, or implementation of a service; False otherwise.</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="applicablePolicies" type="xsd:string">
      <xsd:annotation>
        <xsd:documentation>A collection of all policies that in some way constrain, govern, or control the usage of a service, application, process. etc.</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="approvalRequiredIndicator" type="xsd:string" use="required">
      <xsd:annotation>
        <xsd:documentation>True when a permission must first be obtained prior to using a service or performing some action in a business process; False otherwise.</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="licensingRequiredIndicator" type="xsd:string" use="required">
      <xsd:annotation>
        <xsd:documentation>True when a license is required to use a service or application; False otherwise.</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="serviceResponseTime" type="xsd:string" use="required">
      <xsd:annotation>
        <xsd:documentation>A description of the average response time for a service.  The response time is calculated as the time input is provided to the service until the service completes its process or provides output for the consumer.</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="serviceAverageThroughput" type="xsd:string">
      <xsd:annotation>
        <xsd:documentation>A description of how often a service is expected to be, or actually used, averaged over a period of time.</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="serviceMaximumThroughput" type="xsd:string">
      <xsd:annotation>
        <xsd:documentation>A description of the limit of how often a service is able to be accessed or used at, over a period of time during peak capacity.</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="serviceAvailability" type="xsd:string">
      <xsd:annotation>
        <xsd:documentation>A description or measurement of the expected availbility that a service is usable.</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
  </xsd:complexType>
  <xsd:element name="ServiceLevelAgreement" type="data.grauml.graAnnotationModel:ServiceLevelAgreement">
    <xsd:annotation>
      <xsd:documentation>A collection of policies, agreements, licensing and any other goverenace or performance documentation specifying constraints and any other details regarding the realization of a service.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:complexType name="AmountType">
    <xsd:annotation>
      <xsd:documentation>A data type for an amount of money.</xsd:documentation>
    </xsd:annotation>
    <xsd:choice maxOccurs="unbounded" minOccurs="0">
      <xsd:element ref="xmi:Extension"/>
    </xsd:choice>
    <xsd:attribute ref="xmi:id"/>
    <xsd:attributeGroup ref="xmi:ObjectAttribs"/>
    <xsd:attribute name="currencyText" type="xsd:string">
      <xsd:annotation>
        <xsd:documentation>A unit of money or exchange.</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="value" type="xsd:string" use="required"/>
  </xsd:complexType>
  <xsd:element name="AmountType" type="data.grauml.graAnnotationModel:AmountType">
    <xsd:annotation>
      <xsd:documentation>A data type for an amount of money.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:complexType name="ServiceInterfaceSpecification">
    <xsd:annotation>
      <xsd:documentation>A data type representing details relating to the interface of a service.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="data.grauml.graAnnotationModel:GraServiceAnnotationBase">
        <xsd:choice maxOccurs="unbounded" minOccurs="0">
          <xsd:element name="serviceInteractionProfile" type="data.grauml.graAnnotationModel:ServiceInteractionProfile">
            <xsd:annotation>
              <xsd:documentation>A set of information about the Service Interaction Profile Implemented.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="portDefault" type="data.grauml.graAnnotationModel:Port"/>
          <xsd:element name="interfaceDefault" type="data.grauml.graAnnotationModel:Interface"/>
          <xsd:element name="operationDefault" type="data.grauml.graAnnotationModel:Operation"/>
          <xsd:element name="messageDefault" type="data.grauml.graAnnotationModel:Message"/>
          <xsd:element name="provider" type="data.grauml.graAnnotationModel:Service"/>
        </xsd:choice>
        <xsd:attribute name="securityImplementedIndicator" type="xsd:string" use="required">
          <xsd:annotation>
            <xsd:documentation>True when security has been implemented to access this service.False otherwise.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="securityDescriptionText" type="xsd:string">
          <xsd:annotation>
            <xsd:documentation>A text description that identifies the security which was implemented to protect this service interface (GFIPM, Trusted Broker, etc)</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="uriAddress" type="xsd:string" use="required">
          <xsd:annotation>
            <xsd:documentation>A fully qualified locator of the service interface potentially including version and environment.
[derived from] Element name</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="uriDescription" type="xsd:string">
          <xsd:annotation>
            <xsd:documentation>A description of the URI. This would provide better understanding of what is actually at the URI address.
[derived from] Element description</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:element name="ServiceInterfaceSpecification" type="data.grauml.graAnnotationModel:ServiceInterfaceSpecification">
    <xsd:annotation>
      <xsd:documentation>A data type representing details relating to the interface of a service.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:complexType name="ServiceInteractionProfile">
    <xsd:annotation>
      <xsd:documentation>A data structure containing information about the Service Interaction Profile Implemented</xsd:documentation>
    </xsd:annotation>
    <xsd:choice maxOccurs="unbounded" minOccurs="0">
      <xsd:element ref="xmi:Extension"/>
    </xsd:choice>
    <xsd:attribute ref="xmi:id"/>
    <xsd:attributeGroup ref="xmi:ObjectAttribs"/>
    <xsd:attribute name="sipName" type="xsd:string" use="required">
      <xsd:annotation>
        <xsd:documentation>A name of the Service Interaction Profile Implemented.
[Derived from] SIP Name</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="sipVersion" type="xsd:string" use="required">
      <xsd:annotation>
        <xsd:documentation>A version of the Service Interaction Profile Implemented</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
  </xsd:complexType>
  <xsd:element name="ServiceInteractionProfile" type="data.grauml.graAnnotationModel:ServiceInteractionProfile">
    <xsd:annotation>
      <xsd:documentation>A data structure containing information about the Service Interaction Profile Implemented</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
</xsd:schema>
