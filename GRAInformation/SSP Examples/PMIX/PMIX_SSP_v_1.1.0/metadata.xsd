<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:globaljra="http://it.ojp.gov/gsp/services/1.0.0" targetNamespace="http://it.ojp.gov/gsp/services/1.0.0" elementFormDefault="qualified">
	<xs:complexType name="AgreementType">
		<xs:annotation>
			<xs:documentation>A data structure representing an agreement, formal or informal, associated with a service, application or thing, tangible or otherwise.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:choice>
				<xs:element ref="globaljra:AgreementDescriptionText"/>
				<xs:element ref="globaljra:AgreementURI"/>
			</xs:choice>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AmountType">
		<xs:annotation>
			<xs:documentation>A data type for an amount of money.</xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:extension base="xs:decimal">
				<xs:attribute ref="globaljra:currencyText" use="optional" default="USD"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="BusinessContextType">
		<xs:annotation>
			<xs:documentation>A data structure representing the collection of details describing the business context in which the service operates.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="globaljra:DomainDescription"/>
			<xs:element ref="globaljra:Classifications" minOccurs="0"/>
			<xs:element ref="globaljra:ExchangePartnerTypes" minOccurs="0"/>
			<xs:element ref="globaljra:Endorsements" minOccurs="0"/>
			<xs:element ref="globaljra:Sponsors" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CostType">
		<xs:annotation>
			<xs:documentation>A data structure representing the costs associating with using something.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="globaljra:UsageCostAmount" minOccurs="0"/>
			<xs:element ref="globaljra:UsageUnitCostAmount" minOccurs="0"/>
			<xs:element ref="globaljra:CreationCostAmount" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="DomainDescriptionType">
		<xs:annotation>
			<xs:documentation>A data structure representing the primary domain or line of business (LoB) that a service covers.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="globaljra:DomainNameText"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="EntityType">
		<xs:annotation>
			<xs:documentation>A data structure representing either a person or an organization.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:choice>
				<xs:element name="Organization" type="globaljra:OrganizationType"/>
				<xs:element name="Person" type="globaljra:PersonContactInformationType"/>
			</xs:choice>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="IEPDReferenceType">
		<xs:annotation>
			<xs:documentation>A data structure describing the name and location of an IEPD.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="globaljra:IEPDName" maxOccurs="unbounded"/>
			<xs:element ref="globaljra:IEPDURL" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="OrganizationType">
		<xs:annotation>
			<xs:documentation>A data structure describing a body of people grouped together for a common purpose.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="globaljra:OrganizationName"/>
			<xs:element ref="globaljra:OrganizationAcronym" minOccurs="0"/>
			<xs:element ref="globaljra:OrganizationFullAddressText" minOccurs="0"/>
			<xs:element ref="globaljra:OrganizationWebSiteURL" minOccurs="0"/>
			<xs:element ref="globaljra:OrganizationRoleDescriptionText"/>
			<xs:element ref="globaljra:OrganizationRoleDetailedDescriptionText" minOccurs="0"/>
			<xs:element ref="globaljra:OrganizationPointOfContact" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="PerformanceMetricsType">
		<xs:annotation>
			<xs:documentation>A data type describing a collection of performance amd quality of service (QoS) characteristics that a service is either expected or required to meet once implemented.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="globaljra:PerformanceResponseDuration"/>
			<xs:element ref="globaljra:ServiceAverageThroughput" minOccurs="0"/>
			<xs:element ref="globaljra:ServiceMaximumThroughput" minOccurs="0"/>
			<xs:element ref="globaljra:ServiceAvailability" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="PersonContactInformationType">
		<xs:annotation>
			<xs:documentation>A data structure describing a persons name and the means to contact that person.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="globaljra:ContactPersonName"/>
			<xs:element ref="globaljra:ContactPersonEmailID" minOccurs="0"/>
			<xs:element ref="globaljra:ContactPersonPhoneNumberID"/>
			<xs:element ref="globaljra:ContactPersonAddress" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ServiceActionType">
		<xs:annotation>
			<xs:documentation>A data structure representing a services action information</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="globaljra:ActionName"/>
			<xs:element ref="globaljra:ActionPurpose"/>
			<xs:element ref="globaljra:MessageExchangePattern"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ServiceDescriptionType">
		<xs:annotation>
			<xs:documentation>A data structure representing the details describing a service.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="globaljra:ServicePurposeText"/>
			<xs:element ref="globaljra:ServiceScopeDescriptionText"/>
			<xs:element ref="globaljra:ServiceDescriptionSummaryText" minOccurs="0"/>
			<xs:element ref="globaljra:ServiceDescriptionText" minOccurs="0"/>
			<xs:element ref="globaljra:ServiceDescriptionKeywordText" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="globaljra:ServiceCapabilities"/>
			<xs:element ref="globaljra:ServiceRealWorldEffects"/>
			<xs:sequence minOccurs="0">
				<xs:choice>
					<xs:element ref="globaljra:SecurityClassificationText"/>
					<xs:any namespace="globaljra:SecurityClassificationAny"/>
				</xs:choice>
			</xs:sequence>
			<xs:element ref="globaljra:ServiceDependencies" minOccurs="0"/>
			<xs:element ref="globaljra:IEPDReferences" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ServiceIdentificationType">
		<xs:annotation>
			<xs:documentation>A data structure representing the means of uniquely identifying a service.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="globaljra:ServiceFullNameText" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="globaljra:ServiceID" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="globaljra:ServiceURI" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="globaljra:ServiceNameAbbreviationText" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ServiceInteractionProfileType">
		<xs:annotation>
			<xs:documentation>A data structure containing information about the Service Interaction Profile Implemented</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="globaljra:SIPName"/>
			<xs:element ref="globaljra:SIPVersion"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ServiceInterfaceSpecificationType">
		<xs:annotation>
			<xs:documentation>A data type representing details relating to the interface of a service.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="globaljra:URIDetails"/>
			<xs:element ref="globaljra:ServiceActions" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="globaljra:ServiceInteractionProfiles" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="globaljra:SecurityImplementedIndicator"/>
			<xs:element ref="globaljra:SecurityDescriptionText" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ServiceLevelAgreementsType">
		<xs:annotation>
			<xs:documentation>A collection of policies, agreements, licensing and any other goverenace or performance documentation specifying constraints and any other details regarding the realization of a service.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="globaljra:ServicePolicyAndContracts"/>
			<xs:element ref="globaljra:PerformanceMetrics"/>
			<xs:element ref="globaljra:ServiceUsageDetails"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ServiceMetadataType">
		<xs:annotation>
			<xs:documentation>A data structure representing details and characteristics describing a service.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="globaljra:ServiceIdentification"/>
			<xs:element ref="globaljra:ServiceDescription"/>
			<xs:element ref="globaljra:VersionInformation"/>
			<xs:element ref="globaljra:RelatedOrganizationInformation" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element ref="globaljra:BusinessContext"/>
			<xs:element ref="globaljra:ServiceInterface" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="globaljra:ServiceLevelAgreements"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ServicePolicyAndContractsType">
		<xs:annotation>
			<xs:documentation>A data type describing a collection of policies and other agreement documentation specifying constraints and any other details regarding the realization of a service.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="globaljra:ApplicablePoliciesIndicator"/>
			<xs:element ref="globaljra:ApplicablePolicies" minOccurs="0"/>
			<xs:element ref="globaljra:ApplicableContractsIndicator"/>
			<xs:element ref="globaljra:ApplicableContracts" minOccurs="0"/>
			<xs:element ref="globaljra:ApplicableAgreementsIndicator"/>
			<xs:element ref="globaljra:ApplicableAgreements" minOccurs="0"/>
			<xs:element ref="globaljra:ApplicableUmbrellaAgreementsIndicator"/>
			<xs:element ref="globaljra:ApplicableUmbrellaAgreements" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ServiceUsageType">
		<xs:annotation>
			<xs:documentation>A data type describing a collection of  licensing and any other usage goverenace constraints regarding the realization of a service.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="globaljra:ApprovalRequired"/>
			<xs:element ref="globaljra:LicensingRequired"/>
			<xs:element ref="globaljra:LicensingAgreement" minOccurs="0"/>
			<xs:element name="ServiceCost" type="globaljra:CostType" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="SummaryTextType">
		<xs:annotation>
			<xs:documentation>A string limited to 160 characters for summary descriptivr text.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="160"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="URIDetailType">
		<xs:annotation>
			<xs:documentation>A collection containing URIs information</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="globaljra:URIAddress"/>
			<xs:element ref="globaljra:URIDescription"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="UsageUnitAmountType">
		<xs:annotation>
			<xs:documentation>A data type for an amount of money based on a per unit basis.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="globaljra:AmountType">
				<xs:attribute ref="globaljra:unitMeasureText" use="optional"/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="VersionInformationType">
		<xs:annotation>
			<xs:documentation>A data structure representing a version for something, such as a service, document, etc.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="globaljra:MajorVersion"/>
			<xs:element ref="globaljra:MinorVersion"/>
			<xs:element ref="globaljra:RevisionVersion" minOccurs="0"/>
			<xs:element ref="globaljra:CreationDate"/>
			<xs:element ref="globaljra:ActivationDate" minOccurs="0"/>
			<xs:element ref="globaljra:LastRevisionDate" minOccurs="0"/>
			<xs:element ref="globaljra:NextRevisionDate" minOccurs="0"/>
			<xs:element ref="globaljra:ExpirationDate" minOccurs="0"/>
			<xs:element ref="globaljra:LifecycleStatus"/>
			<xs:element ref="globaljra:AlertsAndNotifications" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<!-- Attributes are defined below -->
	<xs:attribute name="currencyText" type="xs:string">
		<xs:annotation>
			<xs:documentation>A unit of money or exchange.</xs:documentation>
		</xs:annotation>
	</xs:attribute>
	<xs:attribute name="unitMeasureText" type="xs:string">
		<xs:annotation>
			<xs:documentation>A unit of measure in time or quanta.</xs:documentation>
		</xs:annotation>
	</xs:attribute>
	<!-- All elements begin here in alphanumeric ascending order -->
	<xs:element name="ActionName" type="xs:string">
		<xs:annotation>
			<xs:documentation>A service action performed within this service interface.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="ActionPurpose" type="xs:string">
		<xs:annotation>
			<xs:documentation>A description of the Service Action purpose performed by this service interface.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="ActivationDate" type="xs:date">
		<xs:annotation>
			<xs:documentation>A date when a service was or will be first available in production. Do NOT confuse with the date you submitted this service to a registry. The format is YYYY-MM.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="Agreement" type="globaljra:AgreementType">
		<xs:annotation>
			<xs:documentation>A description or reference to an applicable Agreement or MOU governing the use, administration, or implementation of a service.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="AgreementDescriptionText" type="xs:string">
		<xs:annotation>
			<xs:documentation>A free text description of an agreement, formal or informal, associated with a service, application or thing, tangible or otherwise.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="AgreementURI" type="xs:anyURI">
		<xs:annotation>
			<xs:documentation>A locator referencing an agreement, formal or informal, associated with a service, application or thing, tangible or otherwise.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="AlertsAndNotifications" type="xs:anyURI">
		<xs:annotation>
			<xs:documentation>A URL to sign up for alerts and notifications for a specific service</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="ApplicableAgreements">
		<xs:annotation>
			<xs:documentation>A collection of applicable Agreements or MOUs governing the use, administration, or implementation of a service</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="globaljra:Agreement" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="ApplicableAgreementsIndicator" type="xs:boolean">
		<xs:annotation>
			<xs:documentation>True when there are any applicable agreements or Memoranda Of Understanding (MOUs) relating to the use, administration, or implementation of a service; False otherwise.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="ApplicableContracts">
		<xs:annotation>
			<xs:documentation>A collection of any formal contracts associated with a service, application or thing, tangible or otherwise.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="globaljra:Contract" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="ApplicableContractsIndicator" type="xs:boolean">
		<xs:annotation>
			<xs:documentation>True when there are any applicable contracts relating to the use, administration, or implementation of a service; False otherwise.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="ApplicablePolicies">
		<xs:annotation>
			<xs:documentation>A collection of all policies that in some way constrain, govern, or control the usage of a service, application, process. etc.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="globaljra:PolicyText"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="ApplicablePoliciesIndicator" type="xs:boolean">
		<xs:annotation>
			<xs:documentation>True when there are any applicable policies governing the use, administration, or implementation of a service; False otherwise.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="ApplicableUmbrellaAgreements">
		<xs:annotation>
			<xs:documentation>A collection of all umbrella agreements that in some way constrain, govern, or control the usage of a service, application, process. etc.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="globaljra:UmbrellaAgreement" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="ApplicableUmbrellaAgreementsIndicator" type="xs:boolean">
		<xs:annotation>
			<xs:documentation>True when there are any applicable umbrella agreements relating to the use, administration, or implementation of a service; False otherwise.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="ApprovalRequired" type="xs:boolean">
		<xs:annotation>
			<xs:documentation>True when a permission must first be obtained prior to using a service or performing some action in a business process; False otherwise.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="BusinessContext" type="globaljra:BusinessContextType">
		<xs:annotation>
			<xs:documentation>A collection of details describing the business context in which the service operates.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="CapabilityDescriptionText" type="xs:string">
		<xs:annotation>
			<xs:documentation>A free text format description of the capability provided by a service.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="Classification" type="xs:string">
		<xs:annotation>
			<xs:documentation>A classification defining the relationship between a service and an applicable enterprise architecture and business reference model.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="Classifications">
		<xs:annotation>
			<xs:documentation>A collection of classifications defining the relationship between a service and an applicable enterprise architecture and business reference model.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="globaljra:Classification"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="ContactPersonAddress" type="xs:string">
		<xs:annotation>
			<xs:documentation>A physical address of a person in full text form.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="ContactPersonEmailID" type="xs:string">
		<xs:annotation>
			<xs:documentation>An Email of a person.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="ContactPersonName" type="xs:string">
		<xs:annotation>
			<xs:documentation>A name of a person designated as a point of contact.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="ContactPersonPhoneNumberID" type="xs:string">
		<xs:annotation>
			<xs:documentation>A phone number of the person designated as the point of contact for the organization.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="Contract" type="globaljra:AgreementType">
		<xs:annotation>
			<xs:documentation>A formal contract associated with a service, application, process, tranaction, or thing, tangible or otherwise.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="CreationCostAmount" type="globaljra:AmountType">
		<xs:annotation>
			<xs:documentation>A cost create a thing, such as an application or service  This includes the full cost to design, manage, develop, test, implement and maintain.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="CreationDate" type="xs:date">
		<xs:annotation>
			<xs:documentation>A date designation when a service was first created. Do NOT to be confuse with the date a service is submitted to a registry. The format is YYYY-MM.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="DomainDescription" type="globaljra:DomainDescriptionType">
		<xs:annotation>
			<xs:documentation>A primary domain or line of business (LoB) that a service covers.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="DomainNameText" type="xs:string">
		<xs:annotation>
			<xs:documentation>A name of a primary domain or line of business (LoB) that a service covers.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="Endorsements">
		<xs:annotation>
			<xs:documentation>A collection of names and acronyms of professional or governmental organizations or people that endorse this service as an official business exchange.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="globaljra:Endorser" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="Endorser" type="xs:string">
		<xs:annotation>
			<xs:documentation>A name of professional or governmental organizations that endorse this service as an official business exchange.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="ExchangePartnerTypes">
		<xs:annotation>
			<xs:documentation>A collection of the types/categories of organizations that would commonly use a service.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="globaljra:ExchangePartnerTypeDescriptionText"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="ExchangePartnerTypeDescriptionText" type="xs:string">
		<xs:annotation>
			<xs:documentation>A text description for the type/category of organization that would commonly use a service.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="ExpirationDate" type="xs:date">
		<xs:annotation>
			<xs:documentation>A year and month (YYYY-MM) this service is expected to be no longer available (if applicable).</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="IEPDName" type="xs:string">
		<xs:annotation>
			<xs:documentation>A human readable identification of the IEPD</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="IEPDReference" type="globaljra:IEPDReferenceType">
		<xs:annotation>
			<xs:documentation>A set or reference information identifying an Information Exchange Package Document which the service uses in its data model</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="IEPDReferences">
		<xs:annotation>
